#include <locale.h>
#include <iostream>
#include <cmath>
using namespace std;
int main(void) 
{
    int  n;
    double sum,a,i;
    cout << "Enter the number n"<<endl;            //"Введите число n"; 
    cin >> n;
    sum=0.0;
    a=1.0;
        for (i = 1 ;i <=n ; i++)
        {
             sum=sum+(a/i);
             
        }
        cout << "Sum =  " << sum << endl;
    return 0;
}
