
#include <iostream>
#include <math.h>
#include <iomanip>
using namespace std;


int main(void){
    double km,k,mil;
	km=1.6093 ;
	mil=1.0000;
	cin>>k;
		cout <<" milli        km"<<endl;
			for(double i=1.0000;i<=k;i++)
			{	
				if (i<mil*km)
				{
					cout <<fixed<<setprecision(4)<<i/km<<"      "<<i<<endl;
				}
				else
				{
					cout <<fixed<<setprecision(4)<<mil<<"      "<< mil*km<<endl;
					mil++;
				
					if (mil*km>i)
					{
						cout <<fixed<<setprecision(4)<<i/km<<"      "<< i<<endl;
					}
				}	
			
	     	}
    	    
	return 0 ;
}
