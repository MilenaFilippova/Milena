#include <iostream> //Стандартные классы ввода-вывода
#include <math.h> //Различные определения, используемые библиотекой math
#include <locale.h> 
using namespace std;
int main(void) 
{
	
	int n;
    cout <<"Enter the number : ";
    cin>> n;
    if(n!=0)
    {
		  for (int i=abs(n); i>=1; i--)
		  {
			  if ((n%i)==0)  
			  {	
				  cout <<i<< endl; 
			  }
		  }
    }
    else
    {
         cout <<"Data entered incorrectly";
    }
    system("pause");
    
}
