#include <locale.h>
#include <cmath>
#include <iostream>
using namespace std;
int main(void) 
{
    int  a, b, c, d, e, f, k;
    k=0;
    for (a = 0; a<10; a++)
        { 
            for (b = 0; b<10; b++)
            { 
                for (c = 0; c<10; c++)
                {
                    for (d = 0; d<10; d++)
                    { 
                        for (e = 0; e<10; e++)
                        {
                            for (f = 0; f<10; f++) 
                        
                            {
                                if ((a+b+c)==(d+e+f) && (a+b+c)==13) 
                                {
                                    k = k + 1;
                                } 
                            } 
                        } 
                    }
                } 
            }
        }
    cout <<	k << endl;					
	return 0;
}
